/*RadioButton样式设置*/
QRadioButton::indicator {
    width: 60px;
    height: 60px;
}
/*单选框未选中样式*/
QRadioButton::indicator::unchecked {

    image: url(:/icon/check);
}
/*单选框选中样式*/
QRadioButton::indicator::checked {
    image: url(:/icon/check_s);
}
/*RadioButton和checkbox字体和间距设置*/
QRadioButton{
    spacing: 5px;
    font-size: 15px;
}
